@model string

@await Html.PartialAsync("_title","File Viewer")
<script src="~/js/pages/FileViewer.js"></script>
<script>


(async() => 
{
    InitContract();
    await SetFileToView(getIdWithoutVersion("@Model"))

})();
</script>
<div class="text-center">
  <div>
    
      <div class="drag-area active max-width-root">
          <div id="manFileViewer">
              <partial name="_animation"/>
          </div>
      </div>
     <div class="fileTextDiv fileBoxCenter padding-root rounded-root max-width-root">
         <div class="downloadFileInfo">
             <h4 class="font-size-root margin-root" id="SizeFile">Loading</h4>
             <h4 class="font-size-root margin-root" id="Extension">Loading</h4>
             <h4 class="font-size-root margin-root" id="FileName">Loading</h4>
             <button class="btn control" id="DownloadButton">Download</button>
         </div>
     </div>   
  </div>

</div>